    public void loadSettings(EinrichtungsAssistentDialog as) {
        this.as = as;
    }

    public boolean validateSettings() {
        if(this.field_firmenname.getText() == null) {
            JOptionPane.showMessageDialog(null, "Bitte geben Sie einen Unternehmensnamen an.");
            return false;
        }

        return true;
    }

    public void saveSettings() {
        MandantenObj md = new MandantenObj();

        md.setFirmenName(field_firmenname.getText());
        md.setFirmenZusatz(this.field_firmenZusatz.getText());
        md.setStrasse(field_strasse.getText());
        md.setPlz(field_plz.getText());
        md.setOrt(field_ort.getText());
        md.setFirmenRechtsform((String) this.combo_rechtsform.getSelectedItem());
        md.set34c(this.check_34c.isSelected());
        md.set34d(this.check_34d.isSelected());
        md.setIhkRegistriernummer(this.field_ihknummer.getText());

        md.setTelefon(this.field_telefon.getText());
        md.setTelefon2(this.field_telefon2.getText());
        md.setFax(this.field_fax.getText());
        md.setEmail(this.field_email.getText());
        md.setMobil(this.field_mobil.getText());
        md.setHomepage(this.field_internet.getText());

        md.setCreated(new java.sql.Timestamp(System.currentTimeMillis()));
        md.setModified(new java.sql.Timestamp(System.currentTimeMillis()));

        md.setStatus(Status.NORMAL);

        try {
            int id = MandantenSQLMethods.insertIntomandanten(DatabaseConnection.open(), md);
            md.setId(id);

            BasicRegistry.currentMandant = md;
        } catch (SQLException e) {
            Log.databaselogger.fatal("Konnte den Mandanten nicht anlegen", e);
            ShowException.showException("Der Mandant konnte nicht angelegt werden. Unter Details "
                    + "finden Sie weitere Informationen zu dem Fehler",
                    ExceptionDialogGui.LEVEL_FATAL, e,
                    "Schwerwiegend: Konnte Mandanten nicht anlegen");
        }
    }

