    private void btn_backActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btn_backActionPerformed
        if(currentsite == 2) {
            panel_site3.setVisible(false);
            panel_site1.setVisible(true);
            panel_site4.setVisible(false);
            btn_back.setVisible(false);
            label_stepname.setText("Neue Benutzer - Basis Daten");
            currentsite--;
        } else if (currentsite == 3) {
            panel_site3.setVisible(true);
            panel_site1.setVisible(false);
            panel_site4.setVisible(false);
            label_stepname.setText("Neue Benutzer - Weitere Daten");
            btn_next.setText("Weiter");
            currentsite--;
        }

        this.validate();
    }//GEN-LAST:event_btn_backActionPerformed

