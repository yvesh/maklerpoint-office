            this.label_obervermittler.setVisible(true);
        } else {
            this.combo_obervermittler.setVisible(false);
            this.label_obervermittler.setVisible(false);
        }
        this.validate();
    }//GEN-LAST:event_check_untervermittlerActionPerformed

