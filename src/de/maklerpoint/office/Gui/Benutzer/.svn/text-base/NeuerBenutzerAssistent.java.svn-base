    private void btn_nextActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btn_nextActionPerformed
        if(currentsite == 1) {
            panel_site3.setVisible(true);
            panel_site1.setVisible(false);
            panel_site4.setVisible(false);
            btn_back.setVisible(true);
            label_stepname.setText("Neue Benutzer - Weitere Daten");
            currentsite++;
        } else if (currentsite == 2) {
            panel_site3.setVisible(false);
            panel_site1.setVisible(false);
            panel_site4.setVisible(true);
            btn_back.setVisible(true);
            label_stepname.setText("Neue Benutzer - Anmeldedaten");
            btn_next.setText("Beenden");
            currentsite++;
        } else if (currentsite == 3) {
            // Finish it
            try {
                FilesystemBenutzer.createBenutzerDirectory(null);
            } catch (Exception e) {
                
            }

            currentsite = 1;
            this.setVisible(false);
            this.dispose();
        }
        this.validate();
    }//GEN-LAST:event_btn_nextActionPerformed

